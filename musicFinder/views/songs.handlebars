<div class = "container">
    <div class="row">
        <div class="col">
            <div class="container songs-cols">
                <h2>Songs Results</h2>
                <ul>
                {{#each songs}}
                    <li class = "li-item">
                        <a> {{this.name}} </a>
                        <br>
                    <button type="submit" class="btn add-btn" data-id={{this.id}}>Add {{this.name}}</button> 
                    </li>
                {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>