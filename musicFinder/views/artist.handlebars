<div class = "container">
    <div class="row">
        <div class="col">
            <div class="container artist-cols">
                <h2>Artist Results</h2>
                <ul>
                {{#each artists}}
                    <li class = "li-item">
                        Name: {{this.name}}
                    </li>
                    <button type="submit" class="btn add-btn" data-id={{this.id}}>Add {{this.name}}</button> 
                {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>